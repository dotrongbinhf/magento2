<style>
    .page-main {
        max-width: 100%;
        padding: 0px;
    }

    #weather_container {
        border-radius: 10px;
        color: #333;
        font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        display: flex;
        flex-direction: column;
        gap: 10px;
    }

    .day_card {
        background-color: #fff;
        border-radius: 20px;
        overflow: hidden;
        transition-property: all;
        transition-duration: 0.5s;
        transition-timing-function: cubic-bezier(0.075, 0.82, 0.165, 1);
        cursor: pointer;
        border: 1px solid #009879;
        font-size: small;
    }

    .day_card.active {
        flex-grow: 1;
    }

    .day_card.active .content-1 {
        display: none;
    }

    .day_card.active .content-2 {
        display: block;
    }

    .day_card .content-1 {
        display: block;
    }

    .day_card .content-2 {
        display: none;
        background-color: #BBD7EC;
        color: #1F1F1F;
    }

    .card {
        background-color: #fff;
        padding: 10px;
        border-radius: 12px;
        border: 1px solid #009879;
    }

    #wind_direction::after {
        content: '';
        position: absolute;
        left: 50%;
        width: 0;
        height: 0;
        bottom: 0;
        border-top: 10px solid #BBD7EC;
        border-left: 5px solid transparent;
        border-right: 5px solid transparent;
        transform: translateX(-50%);
    }

    .btn {
        cursor: pointer;
        color: #1F1F1F;
        font-weight: bold;
    }

    .btn.inactive {
        color: #1f1f1f6a;
    }
</style>

<div id="weather_container">


    <div style="display: flex;">
        <div style="display: flex; flex-direction: column; gap: 20px; flex-basis: 70%; overflow: hidden; padding: 20px;">
            <div style="display: flex;gap: 12px;font-size: medium;">
                <div id="today_btn" class="btn">Hôm nay</div>
                <div id="7day_btn" class="btn inactive">7 ngày tới</div>
            </div>
            <div id="today_container" class="card" style="display: flex; flex-direction: column; gap: 20px;">

            </div>
            <div id="7day_container" style="display: none;"></div>
            <b>Tổng quan</b>
            <div style="display: flex;gap: 20px;">
                <div class="card" style="display: flex; flex-direction: column; justify-content: space-between;">
                    <b>Lượng mây bao phủ (%)</b>
                    <canvas id="cloud_status"></canvas>
                </div>
                <div class="card"
                    style="display: flex; flex-direction: column;justify-content: space-between;">
                    <b>Chỉ số UV</b>
                    <div style="position: relative; width: 150px; height: 75px; overflow: hidden;">
                        <div style="position: absolute; border-radius: 99999px; background: linear-gradient(to right,#00e5ff92, #0432ff80); width: 150px; height: 150px;"></div>
                        <div style="position: absolute; border-radius: 99999px; background: #fff; width: 130px; height: 130px; left: 50%; transform: translateX(-50%); top: 10px;"></div>
                        <div id="uv_direction" style="width: 40px; height: 4px; background-color: #0432ff80; right: 50%; bottom: 7px; position: absolute; border-radius: 9999px; transform-origin: 100% 100%;">
                            <div style="width: 10px; height: 10px; background-color: #00e5ff; border-radius: 9999px; position: absolute; right: 0; bottom: -3px;"></div>
                        </div>
                    </div>
                    <div style="font-size: small;">Trạng thái: <b id="uv_status"></b></div>
                    <b id="uv_index" style="font-size: x-large; text-align: center;"></b>
                </div>
                <div class="card" style="display: flex; flex-direction: column; justify-content: space-between;">
                    <b>Mặt trời</b>
                    <div style="position: relative; width: 150px; height: 75px;overflow: hidden;">
                        <div style="position: absolute; border-radius: 99999px; background: linear-gradient(to right, #ff974ca0, #1f1f1f5f); width: 140px; height: 140px; top:5px; left: 5px;"></div>
                        <div style="position: absolute; border-radius: 99999px; background: #fff; width: 120px; height: 120px; left: 15px; top: 15px;"></div>
                        <div id="sun_direction" style="position: absolute; width: 144px; height: 72px; background-color: #FFA24C; border-bottom-left-radius: 9999px; border-bottom-right-radius: 9999px; top: 100%; left: 3px; transform-origin: 50% 0;">
                            <div style="position: absolute; border-bottom-left-radius: 9999px; border-bottom-right-radius: 9999px; background: #fff; width: 120px; height: 60px; left: 12px;"></div>
                            <img style="position: absolute; bottom: 100%; transform: translateY(50%); left: -10px;" width="35px" height="35px" src="data:image/svg+xml;base64," alt="">
                        </div>
                    </div>
                    <b style="text-align: center;" id="timeNow"></b>
                    <div>
                        <div style="display: flex; color: #FFA24C; font-size: smaller; justify-content: space-between;">
                            <img width="20px" height="20px" src="data:image/svg+xml;base64," alt="">
                            <img width="20px" height="20px" src="data:image/svg+xml;base64," alt="">
                        </div>
                        <div id="sun_time" style="display: flex; justify-content: space-between;"></div>
                    </div>
                </div>
                <div style="display: flex; flex-direction: column; gap: 20px; justify-content: space-between;"
                    class="card">
                    <b>Độ ẩm</b>
                    <img style="height: 100px;width: 100px; margin: auto;" src="data:image/png;base64," />
                    <div id="humidity" style="display: flex; gap: 20px; justify-content: space-between; align-items: center;"></div>
                </div>
            </div>
            <div style="display: flex; gap: 20px;">
                <div class="card" style="display: flex; justify-content: space-between; flex-direction: column;">
                    <div><b>Tầm nhìn xa</b></div>
                    <div style="display: flex; gap: 30px;">
                        <img width="80px" height="80px" src="data:image/svg+xml;base64,">
                        <div>
                            <div> <b id="visibility"></b></div>
                            <div>Sương mù sẽ ảnh hưởng đến tầm nhìn xa</div>
                        </div>
                    </div>
                </div>
                <div class="card" style="display: flex; flex-direction: column; justify-content: center; gap: 20px;">
                    <div>
                        <div style="display: flex; justify-content: space-between; font-size: x-small; margin-bottom: 5px;">
                            <div>0°C</div>
                            <div>25°C</div>
                            <div>50°C</div>
                        </div>
                        <div style="height: 8px; background-color: #E5E5E5; border-radius: 12px; position: relative; overflow: hidden;">
                            <div id="feel_likes_chart" style="position: absolute; height: 100%; background-color: #FFA24C; width: 50%; border-radius: 12px;"></div>
                        </div>
                    </div>
                    <div style="display: flex;gap: 20px; justify-content: space-between; align-items: center; flex-grow:1">
                        <img width="30px" height="30px" src="data:image/svg+xml;base64," alt="">
                        <div>Cảm thấy như <b id="feel_likes"></b></div>
                    </div>
                </div>
                <div style="display: flex;justify-content: space-between; gap: 20px; align-items: center;"
                    class="card">
                    <div>
                        <p><b>Gió</b></p>
                        <div id="wind_speed"></div>
                    </div>
                    <div
                        style="position: relative; background-color:#BBD7EC;width: 120px; height: 120px; border-radius: 99999px; flex-shrink: 0;">
                        <div
                            style="position: absolute; top: 50%; left: 50%; width: 95%; height: 95%; border-radius: 99999px; background-color:#fff; transform: translate(-50%, -50%);">
                        </div>
                        <div id="wind_direction"
                            style="position: absolute;top: 50%; left: 50%;height: 70%; width: 2px;background-color:#BBD7EC;  transform-origin: center;">
                        </div>
                        <b id="wind_speed_value"
                            style="font-size:x-small;color:#0F0F11; text-align:center; align-content: center; position: absolute; top: 50%; left: 50%; width: 40%; height: 40%; border-radius: 99999px; background-color:#BBD7EC; transform: translate(-50%, -50%);"></b>
                        <div style="position: absolute;font-size:smaller;top: 5px; left: 50%; transform: translate(-50%);">N</div>
                        <div style="position: absolute;font-size:smaller;right: 5px; top: 50%; transform: translate(0, -50%);">E</div>
                        <div style="position: absolute;font-size:smaller;bottom: 5px; left: 50%; transform: translate(-50%);">S</div>
                        <div style="position: absolute;font-size:smaller;left: 5px; top: 50%; transform: translate(0, -50%);">W</div>
                    </div>
                </div>
            </div>
        </div>
        <div style="flex-basis: 30%; display: flex;flex-direction: column; gap:20px; padding: 20px">
            <div class="card" style="flex-grow: 1; display: flex; gap: 20px; justify-content: space-between; color: #E5E5E5; flex-direction: column; background-image: url(https://images.unsplash.com/photo-1507752533523-5186b0bc4c43?q=80&w=1771&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D); background-size: cover; background-position:center">
                <div id="city" style="display: flex; gap: 40px; justify-content: space-between;"></div>
                <div style="display: flex; gap: 20px; justify-content: end;">
                    <input type="text" placeholder="Nhập tên thành phố" id="city_input">
                    <button style="flex-shrink: 0;" onclick="searchCity()">Tìm kiếm</button>
                </div>
            </div>
            <b><div>Nhiệt độ</div></b>
            <canvas class="card" id="temperature"></canvas>
            <b><div>Độ ẩm</div></b>
            <canvas class="card" id="humidity_chart"></canvas>
        </div>
    </div>
</div>

<script>
    let temperature_chart, humidity_chart, cloud_status_chart

    async function success(pos) {
        const container = document.getElementById('weather_container')
        const timeNow = Date.now()
        const dateNow = new Date(timeNow)

        const crd = pos.coords

        const address = await fetch(`https://api.geoapify.com/v1/geocode/reverse?format=json&lat=${crd.latitude}&lon=${crd.longitude}&apiKey=50f62915e98747549de26641d979cc59`).then(res => res.json())

        const res = await fetch(`https://api.openweathermap.org/data/3.0/onecall?lat=${crd.latitude}&lon=${crd.longitude}&appid=c3b4a12051073e8e3c6c4c4baf1f359a&units=metric&lang=vi`).then(res => res.json())


        require(['chartjs'], function(Chart) {
            temperature_chart = new Chart(document.getElementById('temperature'), {
                type: 'line',
                data: {
                    labels: res.hourly.map(e => new Date(e.dt * 1000).toLocaleTimeString('vi-VI', {
                        timeZone: res.timezone,
                        timeStyle: "short"
                    })),
                    datasets: [{
                        label: 'Nhiệt độ (°C)',
                        data: res.hourly.map(e => e.temp),
                        borderWidth: 0.5,
                        backgroundColor: ['#009879'],
                    }]
                },
                options: {
                    animations: {
                        tension: {
                            duration: 1000,
                            easing: 'linear',
                            from: 1,
                            to: 0,
                            loop: true
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: true
                        }
                    }
                }
            });

            humidity_chart = new Chart(document.getElementById('humidity_chart'), {
                type: 'line',
                data: {
                    labels: res.hourly.map(e => new Date(e.dt * 1000).toLocaleTimeString('vi-VI', {
                        timeZone: res.timezone,
                        timeStyle: "short"
                    })),
                    datasets: [{
                        label: 'Độ ẩm (%)',
                        data: res.hourly.map(e => e.humidity),
                        borderWidth: 0.5,
                        backgroundColor: ['#009879'],
                    }]
                },
                options: {

                    scales: {
                        y: {
                            min: 0,
                            max: 100
                        }
                    }
                }
            });

            cloud_status_chart = new Chart(document.getElementById('cloud_status'), {
                type: 'bar',
                data: {
                    labels: res.hourly.map(e => new Date(e.dt * 1000).toLocaleTimeString('vi-VI', {
                        timeZone: res.timezone,
                        timeStyle: "short"
                    })),
                    datasets: [{
                        data: res.hourly.map(e => e.clouds),
                        borderWidth: 0.5,
                        label: 'Lượng mây',
                    }],
                    backgroundColor: new Array(res.hourly.length).fill('#009879'),
                },
                options: {
                    scales: {
                        y: {
                            min: 0,
                            max: 100
                        },
                    },
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            })
        });

        const localeTimeString = dateNow.toLocaleTimeString('vi-VI', {
            timeZone: res.timezone,
            timeStyle: 'short'
        })

        document.getElementById("7day_container").innerHTML = `<div  style="display: flex; gap: 10px;">
            ${res.daily.reduce((prev, cur) => {
            const time = new Date(cur.dt * 1000);
            return prev + `
                <div class="day_card">
                <div class="content-1">
                    <div
                    style="display: flex;flex-direction: column; justify-content: space-between; align-items: center; height: 100%;">
                    <b style="padding: 10px;">${time.getDate()}/${time.getMonth() + 1}</b>
                    <img style="width: 50px;height: 50px;" src='https://openweathermap.org/img/wn/${cur.weather[0].icon}@2x.png' />
                    <div style="padding:20px; font-size:large; display:flex; flex-direction:column; align-items: center">
                        <b>${Math.round(cur.temp.min)}°C</b>
                        <div>-</div>
                        <b>${Math.round(cur.temp.max)}°C</b>
                    </div>
                    </div>
                </div>
                <div class="content-2">
                    <b style="display: flex; background-color: #AECADF; justify-content: space-between; padding: 10px;">
                    <div>${time.getDate()}/${time.getMonth() + 1}</div>
                    <div>${time.toLocaleTimeString('vi-VI', {timeZone: res.timezone, timeStyle: "short" })}</div>
                    </b>
                    <div style="display: flex; align-items: center; justify-content: space-between;padding: 10px;">
                    <div>
                        <div><b style="font-size:xx-large">${Math.round(cur.temp.day)}°C</b></div>
                        <b>${Math.round(cur.temp.min)}°C-${Math.round(cur.temp.max)}°C</b>
                        </div>
                    <div>
                        <img style="width: 50px;height: 50px;" src='https://openweathermap.org/img/wn/${cur.weather[0].icon}@2x.png' />
                        <div><b>${cur.weather[0].description}</b></div>
                    </div>
                    </div>
                    <div style="padding: 10px; align-items: center; display: flex; justify-content: space-between;">
                    <div>
                        <div>Cảm thấy như <b>${Math.round(cur.feels_like.day)}°C</b></div>
                        <div>Tốc độ gió <b>${cur.wind_speed}km/h</b></div>
                        <div>Áp suất <b>${cur.pressure}hPa</b></div>
                        <div>Độ ẩm  <b>${cur.humidity}%</b></div>
                    </div>
                    <div>
                        <div>Mặt trời mọc <b>${new Date(cur.sunrise * 1000).toLocaleTimeString('vi-VI', {timeZone: res.timezone, timeStyle: 'short' })}</b></div>
                        <div>Mặt trời lặn <b>${new Date(cur.sunset * 1000).toLocaleTimeString('vi-VI', {timeZone: res.timezone, timeStyle: 'short' })}</b></div>
                        <div>Mây bao phủ <b>${cur.clouds}%</b></div>
                    </div>
                    </div>
                </div>
                </div>
            `}, '')}
        </div>`



        document.getElementById('today_container').innerHTML = ` 
            <b style="font-size:small">${dateNow.toLocaleDateString('vi-VI', {timeZone: res.timezone, weekday: 'long', day: 'numeric', month: 'long'})}: ${res.daily[0].summary}</b>
        `


        const hourly_container = document.createElement('div')
        hourly_container.style.display = 'flex'
        hourly_container.style.gap = '20px'
        hourly_container.style.maxWidth = '100%'
        hourly_container.style.overflowX = 'auto'
        hourly_container.style.padding = '10px'

        let startX = 0,
            down = false,
            scrollLeft = 0

        hourly_container.addEventListener('mousedown', e => {
            startX = e.clientX
            down = true
            scrollLeft = hourly_container.scrollLeft
        })

        hourly_container.addEventListener('mousemove', e => {
            if (down) {
                hourly_container.scrollLeft = scrollLeft + (startX - e.clientX) * 2
            }
        })

        hourly_container.addEventListener('mouseup', e => {
            down = false
        })

        hourly_container.addEventListener('mouseleave', e => {
            down = false
        })


        hourly_container.innerHTML = res.hourly.reduce((prev, cur) => {
            return prev + `
                <div style="display:flex; flex-direction:column; align-items:center; user-select:none; text-wrap:nowrap">
                    <b>${new Date(cur.dt * 1000).toLocaleTimeString('vi-VI', {timeZone: res.timezone, timeStyle: 'short' })}</b>
                    <img draggable="false" style="width: 80px;height: 80px;" src='https://openweathermap.org/img/wn/${cur.weather[0].icon}.png' />
                    <b>${Math.round(cur.temp)}°C</b>
                    <div style="font-size:small">${cur.weather[0].description}</div>
                    <div style="font-size:small">xác suất mưa <b>${Math.round(cur.pop * 100)}%</b></div>
                </div>
            `
        }, '')

        document.getElementById('today_container').appendChild(hourly_container)

        document.querySelectorAll('.day_card').forEach((card, i) => {
            if (i === 0) {
                card.classList.add('active')
            }
            card.addEventListener('click', e => {
                document.querySelectorAll('.day_card').forEach(card => {
                    card.classList.remove('active')
                })
                e.target.closest('.day_card').classList.add('active')
            })
        })

        document.getElementById('humidity').innerHTML = `<b style="font-size:xx-large">${res.current.humidity}%</b>
        <div style="font-size:small">Nhiệt độ khí quyển là <b>${res.current.dew_point}°C</b></div>`

        document.getElementById('visibility').innerText = `${res.current.visibility}km`
        document.getElementById('feel_likes').innerText = `${Math.round(res.current.feels_like)}°C`
        document.getElementById('feel_likes_chart').style.width = `${res.current.feels_like * 100 / 50}%`

        document.getElementById('sun_time').innerHTML = `<b>${new Date(res.current.sunrise * 1000).toLocaleTimeString('vi-VI', {timeZone: res.timezone, timeStyle: 'short' })}</b>
        <b>${new Date(res.current.sunset * 1000).toLocaleTimeString('vi-VI', {timeZone: res.timezone, timeStyle: 'short' })}</b>`
        document.getElementById('timeNow').innerHTML = `${dateNow.toLocaleTimeString('vi-VI', {timeZone: res.timezone, timeStyle: 'short' })}`
        document.getElementById('sun_direction').style.rotate = timeNow / 1000 < res.current.sunrise ? '0deg' : (timeNow / 1000 > res.current.sunset) ? '180deg' : (timeNow / 1000 - res.current.sunrise) / (res.current.sunset - res.current.sunrise) * 180 + 'deg'

        document.getElementById('uv_index').innerText = `${res.current.uvi}/10`
        document.getElementById('uv_direction').style.rotate = `${res.current.uvi * 18}deg`
        document.getElementById('uv_status').innerHTML = res.current.uvi > 7 ? 'Cao' : res.current.uvi > 5 ? 'Trung bình' : 'Thấp'

        {
            let windDirection = '';
            if (res.current.wind_deg >= 0 && res.current.wind_deg < 22.5) {
                windDirection = 'Bắc';
            } else if (res.current.wind_deg >= 22.5 && res.current.wind_deg < 67.5) {
                windDirection = 'Đông Bắc';
            } else if (res.current.wind_deg >= 67.5 && res.current.wind_deg < 112.5) {
                windDirection = 'Đông';
            } else if (res.current.wind_deg >= 112.5 && res.current.wind_deg < 157.5) {
                windDirection = 'Đông Nam';
            } else if (res.current.wind_deg >= 157.5 && res.current.wind_deg < 202.5) {
                windDirection = 'Nam';
            } else if (res.current.wind_deg >= 202.5 && res.current.wind_deg < 247.5) {
                windDirection = 'Tây Nam';
            } else if (res.current.wind_deg >= 247.5 && res.current.wind_deg < 292.5) {
                windDirection = 'Tây';
            } else if (res.current.wind_deg >= 292.5 && res.current.wind_deg < 337.5) {
                windDirection = 'Tây Bắc';
            } else if (res.current.wind_deg >= 337.5 && res.current.wind_deg <= 360) {
                windDirection = 'Bắc';
            }
            document.getElementById('wind_speed').innerHTML = `Tốc độ gió hiện tại là <b>${res.current.wind_speed}km/h</b> từ phía ${windDirection}.<br>${res.current.wind_gust ? `Tốc độ gió giật lên đến <b>${res.current.wind_gust}km/h`: ''}</b>`
            document.getElementById('wind_speed_value').innerHTML = `${res.current.wind_speed}<br>km/h`
            document.getElementById('wind_direction').style.transform = `translate(-50%, -50%) rotate(${res.current.wind_deg}deg)`
        }

        document.getElementById('city').innerHTML = `
            <div>
                <b>${address.results[0].address_line1}</b>
                <div style="font-size:smaller">${address.results[0].formatted} </div>
                <div style="font-size:smaller">${dateNow.toLocaleString('vi-VI', {timeZone: res.timezone})} </div>
            </div> 
            <div>
                <div style="font-size:xx-large; text-align: end;"><b>${Math.round(res.current.temp)}°C</b></div>
            </div>
        `

        document.getElementById('today_btn').onclick = e => {
            if (e.target.classList.contains('inactive')) {
                e.target.classList.remove('inactive')
                document.getElementById('7day_container').style.display = 'none'
                document.getElementById('today_container').style.display = 'block'
                document.getElementById('7day_btn').classList.add('inactive')
            }
        }

        document.getElementById('7day_btn').onclick = e => {
            if (e.target.classList.contains('inactive')) {
                e.target.classList.remove('inactive')
                document.getElementById('today_container').style.display = 'none'
                document.getElementById('7day_container').style.display = 'block'
                document.getElementById('today_btn').classList.add('inactive')
            }
        }
    }

    async function init() {

        function error(err) {
            console.warn(`ERROR(${err.code}): ${err.message}`);
        }

        navigator.geolocation.getCurrentPosition(success, error);
        // success({
        //     coords: {
        //         latitude: "21.031467399999997",
        //         longitude: "105.80163809695465"
        //     }
        // })
    }


    function clean() {
        try {
            temperature_chart.destroy()
            humidity_chart.destroy()
            cloud_status_chart.destroy()
        } catch(e) {
            console.log(e)
        }
    }

    function searchCity() {
        fetch(`https://api.geoapify.com/v1/geocode/search?text=${document.getElementById('city_input').value}&format=json&limit=1&apiKey=50f62915e98747549de26641d979cc59`)
            .then(res => res.json())
            .then(res => {
                clean()
                success({
                    coords: {
                        latitude: res.results[0].lat,
                        longitude: res.results[0].lon
                    }
                })
            })
    }

    document.addEventListener("DOMContentLoaded", init)
</script>